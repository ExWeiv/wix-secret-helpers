{
    "compilerOptions": {
        "target": "ES2020",
        "module": "CommonJS",
        "outDir": "./lib",
        "esModuleInterop": true,
        "forceConsistentCasingInFileNames": true,
        "strict": true,
        "noUnusedLocals": true,
        "noUnusedParameters": true,
        "noImplicitReturns": true,
        "skipLibCheck": true,
        "declaration": true,
        "sourceMap": true,
        "removeComments": true
    },
    "include": [
        "index.ts"
    ],
    "exclude": []
}