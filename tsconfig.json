{
    "compilerOptions": {
        "target": "ES2020",
        "lib": [
            "ES2020",
        ],
        "module": "CommonJS",
        "outDir": "./lib",
        "removeComments": true,
        "esModuleInterop": true,
        "forceConsistentCasingInFileNames": true,
        "strict": true,
        "noUnusedLocals": true,
        "noUnusedParameters": true,
        "noImplicitReturns": true,
        "skipLibCheck": true
    },
    "include": [
        "./src/**/*"
    ]
}